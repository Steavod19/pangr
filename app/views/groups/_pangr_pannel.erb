<div class="group-callout">
<ul class="example-orbit-content" data-orbit>
  <% @groups.each do |group| %>
  <li data-orbit-slide="headline-1">
    <div class="box-spacing">
      <h2><%= link_to group.title, group_path(group) %></h2>
      <p><%= group.description %></p>
    </div>
  </li>
  <% end %>
</ul>
</div>
