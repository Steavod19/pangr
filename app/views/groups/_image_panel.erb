<div class="panel image-panel image-row ">
  <p id="recent-title" class="text-center panel-title-white">Recently Added Images</p>

  <div  >
    <ul id="random-image" class="medium-block-grid-4 medium-centered">
      <% @images.each do |post| %>
          <li  class="block-image"><%= link_to image_tag(post.post_photo.panel), group_path(post.group) %></li>
      <% end %>
    </ul>
  </div>

</div>
