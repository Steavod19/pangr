<div class="panel image-panel">
  <div class="large-12 columns">

    <ul class="medium-block-grid-4 medium-centered">
      <% @images.each do |post| %>
          <li class="block-image"><%= link_to image_tag(post.post_photo.panel), group_path(post.group) %></li>
      <% end %>
    </ul>
  </div>
</div>
